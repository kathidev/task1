pipeline
{

    agent any

    tools {
           
	  maven 'maven'

          java 'java'

	}
    stages
	{

         stage('chekoutstage')
      {
         steps
         {
            
            checkout scm
         }
      }
       stage('ContinuousBuild') 
      {
       steps
       {
        sh 'mvn package'
      } 
    }
 }	
}
